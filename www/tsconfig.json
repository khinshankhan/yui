{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "plugins": [{ "name": "next" }]
  },
  "include": [".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "**/*.mjs"],
  "exclude": ["node_modules", ".next"]
}
